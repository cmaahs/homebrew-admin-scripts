#!/usr/bin/env bash

HACK_DATE=$(gdate -d "tue" +"%Y-%m-%d")

if [[ ! -f "/Users/cmaahs/Google Drive/src/Markdown/personal-notes/SpliceMachine/Hackathon/Hack_${HACK_DATE}.md" ]]; then
